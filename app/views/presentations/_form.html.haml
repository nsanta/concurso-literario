= form_for [@course,@presentation], :html => { :class => 'form-horizontal' } do |f|
  .control-group
    = f.label :asset, "Documento", :class => 'control-label'
    .controls
      = f.file_field :asset, :class => 'text_field'
  .form-actions
    = f.submit "Enviar", :class => 'btn btn-primary'
    = link_to "Cancelar", course_path(@course), :class => 'btn'
